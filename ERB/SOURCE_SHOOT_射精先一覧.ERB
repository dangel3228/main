;-------------------------------------------------------------------------------------------
;SHOOT_2:口
;-------------------------------------------------------------------------------------------
@SHOOT_NAME_2
TSTR:コマンド名受渡 = 口内射精
RETURN 1

@SHOOT_PALAM_2(ARG)

CALL SOURCE_CALC_快Ｓ(TARGET:ARG, PLAYER, 100)
CALL SOURCE_CALC_接触(TARGET:ARG, PLAYER, 350)

EXP:Ｓ経験 += 5
LOCALS = Ｓ経験
PRINTFORM %LOCALS,15,LEFT%
LOCALS = +5
PRINTFORML %LOCALS,4,LEFT%(%CALLNAME:(TARGET:ARG)%)

EXP:精液経験 += TFLAG:フィニッシュフラグ
LOCALS = 精液経験
PRINTFORM %LOCALS,15,LEFT%
LOCALS = +{TFLAG:フィニッシュフラグ}
PRINTFORML %LOCALS,4,LEFT%(%CALLNAME:(TARGET:ARG)%)

;ここで精飲するかどうかをTCVAR:ARG:精飲経験で渡す
IF (TALENT:ARG:恋慕 || TALENT:ARG:セフレ || CFLAG:ARG:性愛度 >= 経験値テーブル:(関係閾値:4)) && EXP:ARG:口淫経験 + EXP:ARG:精飲経験 >= 60
	;喜んで飲む
	TCVAR:ARG:精飲経験 = 2
ELSEIF TALENT:ARG:恋慕 || TALENT:ARG:セフレ || CFLAG:ARG:性愛度 >= 経験値テーブル:(関係閾値:3) || TALENT:ARG:性的興味 > 0 || EXP:ARG:口淫経験 >= 30
	;なんとか飲み込む
	TCVAR:ARG:精飲経験 = 1
ELSE
	;吐き出す
	TCVAR:ARG:精飲経験 = 0
ENDIF


@SHOOT_ABLE_2(ARG)
;選択肢に出る条件
;フェラチオ系モードじゃない時はアウト
SIF MODE_確認("フェラチオ系", TARGET:ARG) == 0 && MODE_確認("シックスナイン", TARGET:ARG) == 0
	RETURN 0
RETURN 1

@MESSAGE_SHOOT_2(ARG)
PRINTFORM %CALLNAME:PLAYER%は%CALLNAME:(TARGET:ARG)%の口に
SIF TFLAG:フィニッシュフラグ > 1
	PRINT 大量の
PRINT 精液を吐き出した…

;-------------------------------------------------------------------------------------------
;SHOOT_3:乳内
;-------------------------------------------------------------------------------------------
@SHOOT_NAME_3
TSTR:コマンド名受渡 = 乳内射精
SIF TALENT:TARGET:バストサイズ < 1
	TSTR:コマンド名受渡 = 胸に射精
RETURN 1

@SHOOT_PALAM_3(ARG)

CALL SOURCE_CALC_快Ｂ(TARGET:ARG, PLAYER, 10)
CALL SOURCE_CALC_接触(TARGET:ARG, PLAYER, 350)

EXP:Ｂ経験 += 1
LOCALS = Ｂ経験
PRINTFORM %LOCALS,15,LEFT%
LOCALS = +1
PRINTFORML %LOCALS,4,LEFT%(%CALLNAME:(TARGET:ARG)%)

EXP:精液経験 += TFLAG:フィニッシュフラグ
LOCALS = 精液経験
PRINTFORM %LOCALS,15,LEFT%
LOCALS = +{TFLAG:フィニッシュフラグ}
PRINTFORML %LOCALS,4,LEFT%(%CALLNAME:(TARGET:ARG)%)

@SHOOT_ABLE_3(ARG)
;選択肢に出る条件
;パイズリ系モードじゃない時はアウト
SIF MODE_確認("パイズリ系", TARGET:ARG) == 0
	RETURN 0
RETURN 1

@MESSAGE_SHOOT_3(ARG)

PRINTFORM %CALLNAME:PLAYER%は%CALLNAME:(TARGET:ARG)%の
SELECTCASE TALENT:(TARGET:ARG):バストサイズ
	CASE IS < 1
		PRINT 胸に
	CASE 1
		PRINT 乳の谷間に
	CASE 2
		PRINT 大きな乳の谷間に
	CASE 3
		PRINT とても豊かに育った乳の谷間に
ENDSELECT
SIF TFLAG:フィニッシュフラグ > 1
	PRINT 大量の
PRINT 精液を吐き出した…

